<%= simple_form_for trip do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <trix-editor input="trip_description"></trix-editor>
  <%= f.input :destination %>
  <%= f.input :price %>
  <%= f.simple_fields_for :photos, trip.photos do |p| %>
    <%= p.input :url, as: :file, input_html: { multiple: true } %>
    <%= p.input :url_cache, as: :hidden %>
  <% end %>
  <%= f.submit %>
<% end %>
